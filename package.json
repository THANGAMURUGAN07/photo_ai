{
  "$schema": "http://json.schemastore.org/package",
  "name": "photographer-event-manager",
  "version": "2.0.0",
  "description": "Event photo management system with face recognition and automated delivery",
  "main": "app.js",
  "scripts": {
    "start1": "node app.js",
    "start": "node app-cloud.js",
    "dev1": "nodemon app.js",
    "dev": "nodemon app-cloud.js",
    "setup": "npm install && python -m pip install -r requirements.txt",
    "test": "jest",
    "test:cloud": "node test-cloud-storage.js",
    "migrate": "node migrate-to-cloud.js",
    "migrate:dry-run": "node migrate-to-cloud.js --dry-run",
    "lint": "eslint ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/s3-request-presigner": "^3.515.0",
    "@google-cloud/storage": "^7.7.0",
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "megajs": "^1.1.1",
    "mongoose": "^7.8.7",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "qrcode": "^1.5.4",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
